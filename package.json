{
  "name": "wasm-module",
  "displayName": "WASM Module",
  "description": "WASM Module",
  "version": "1.0.0",
  "preview": true,
  "private": true,
  "publisher": "Arm",
  "author": "Rob Moran <rob.moran@arm.com>",
  "license": "UNLICENSED",
  "browser": "dist/browser.js",
  "repository": "https://github.com/thegecko/vscode-wasm-module",
  "engines": {
    "vscode": "^1.63.0"
  },
  "scripts": {
    "prepare": "yarn run build",
    "clean": "git clean -f -x ./node_modules ./dist ./*.wasm",
    "build": "yarn run compile && webpack --mode production",
    "compile": "./build.sh",
    "watch": "webpack -w",
    "package": "vsce package",
    "serve": "serve --cors -p 11100"
  },
  "dependencies": {
    "@vscode/wasm-wasi": "0.13.0-pre.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.63.0",
    "@vscode/vsce": "^2.19.0",
    "serve": "^14.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "extensionDependencies": [
		"ms-vscode.wasm-wasi-core"
	],
  "contributes": {
    "commands": [
      {
        "command": "wasm.test",
        "title": "Test WASM"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "vsce": {
    "yarn": true
  }
}
